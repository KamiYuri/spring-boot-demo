{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "allowJs": true,
      "jsx": "preserve",
      "sourceMap": true,
      "lib": ["esnext", "dom"],
      "plugins": [{ "name": "@vuedx/typescript-plugin-vue" }]
    },
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
  }